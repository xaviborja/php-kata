{
    "name": "kata/php",
    "description": "PHP Kata skeleton",
    "keywords": ["kata","TDD"],
    "homepage": "https://github.com/xaviborja/php-kata",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev"
}
